<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>@ViewData["Title"] - Telegin</title>
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
        <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/Telegin.styles.css" asp-append-version="true" />

        <link rel="stylesheet" href="~/font-awesome/css/fontawesome.css" />
        <link rel="stylesheet" href="~/font-awesome/css/fontawesome.min.css" />
        <link rel="stylesheet" href="~/font-awesome/css/solid.css" />
        <link rel="stylesheet" href="~/css/CheckboxTeleginExample.css" />
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap-grid.css" />
        <link rel="stylesheet" href="~/font-awesome/css/all.css"/>
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css"/>
        <link rel="stylesheet" href="~/font-awesome/sprites/solid.svg"/>

    
    </head>
    <body>
        <header>
            <nav class="navbar navbar-expand-md navbar-dark bg-dark">

            @* Here begin menu part1 _MenuPartial.cshtml *@
            @await Html.PartialAsync("_MenuPartial")
            @* Here the end menu part1 _MenuPartial.cshtml *@
                <!-- меню сайта - конец -->

                <!-- Информация пользователя -->
            @* Here begin menu part2 _MenuPartial.cshtml *@
            @await Html.PartialAsync("_UserInfoPartial")
            @* Here the end menu part2 _MenuPartial.cshtml *@
            
            </nav>
    
        </header>
        <div class="container">
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
        </div>



        <footer class="border-top footer text-muted">
            <div class="navbar navbar-dark navbar-expand-md bg-dark">
                <div class="navbar-nav ms-auto">
                    <a class="nav-item nav-link" href="http://www.facebook.com">
                        <i class="fa-brands fa-facebook-f fa-2x"></i>
                    </a>
                    <a class="nav-item nav-link" href="http://www.vk.com">
                        <i class="fa-brands fa-vk fa-2x"></i>
                    </a>
                    <a class="nav-item nav-link" href="http://www.twitter.com">
                        <i class="fa-brands fa-twitter fa-2x"></i>
                    </a>
                </div>
            </div>
        </footer>
    
    
    
        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
        @await RenderSectionAsync("Scripts", required: false)
    </body>
</html>
